is.value === \"2\") {\r\n        resp = await getMyContrib(2, 1, 10);\r\n      }\r\n      console.log(resp);\r\n      this.tableData = resp.rows;\r\n      this.total = resp.total;\r\n      // console.log(resp);\r\n    },\r\n    handleCurrentChange(page) {\r\n      // console.log(page);\r\n      this.page = page;\r\n      this.fetchData();\r\n    },\r\n    handlePrevClick() {\r\n      this.page -= 1;\r\n      this.fetchData();\r\n    },\r\n    handleNextClick() {\r\n      this.page += 1;\r\n      this.fetchData();\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.my-contrib-container {\r\n  padding: 20px;\r\n  .list {\r\n    margin-bottom: 5px;\r\n  }\r\n}\r\n</style>"]}]}